{
    "name": "myReadings",
    "description" : "A Chrome extension that logs things I read",
    "version": "1.0",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icons/icon_myReadings_light.png"
    },
    "permissions": [ 
        "storage",
        "tabs"
    ]
  }